<%- include('header'); -%> 


    <div class="box" id="heading">
        <h1>
            <%= listTittle %>
        </h1>
    </div>
    
    <div class="box">
        <% for(let i=0; i<newListItems.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                    <p><%=newListItems[i]%></p>
            </div>
            <% } %>

            <form class="item" action="/" method="post">
                <input type="text" name="newItem" placeholder="Type In a New Item" autocomplete="off">
                <button type="submit" name="list" value=<%= listTittle %>>+</button>
            </form>
    </div>
    
    <%- include('footer'); -%>

  
    
